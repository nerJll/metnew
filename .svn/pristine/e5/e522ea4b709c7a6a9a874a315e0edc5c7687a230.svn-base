<#assign ctx=request.contextPath />
<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<base id="ctx" href="ctx">
<title>会议详情</title>
<meta name="renderer" content="webkit">
<meta http-equiv="X-UA-Compatible" content="IE=Edge,chrome=1">
<meta name="viewport" content="width=device-width, initial-scale=1">
<link rel="stylesheet" type="text/css" href="${ctx}/bootstrap/css/bootstrap.min.css">
<script type="text/javascript" src="${ctx}/easyui/jquery.min.js"></script>
<script type="text/javascript" src="${ctx}/bootstrap/js/bootstrap.min.js"></script>
<script>
function fanhui(){
	window.location.href='${ctx}/meet/showMyApply'
}
</script>
<style>
#header {
	height: 50px;
	background-color: #FCFCFC;
	font-size: 25px;
	text-align: center;
	padding-top: 10px;
}
</style>
</head>
<body>
	<div id='header' class='container-fluid'>
		<span>会议详情</span>
		<input style='float:right' onclick='fanhui()' class="btn btn-default" type="button" value="返回">
	</div>
	<div class='container'>
		<table id="tb" class="table table-bordered">
			<tr class="info">
				<td style="text-align: left;" colspan="6">&nbsp;&nbsp;&nbsp;基本信息</td>
			</tr>
			<tr>
				<td width=5%>发起人</td>
				<td width=10%>${meet.applyEmpName} <input type='hidden'
					id="meetId" name='meetId' value='${meet.meetId}'> <input
					type='hidden' id='applyEmpName' value='${meet.applyEmpName}'></td>
				<td width=5%>主题</td>
				<td width=8%>${meet.meetTheme} <input type='hidden'
					id='meetTheme' value='${meet.meetTheme}'>
				</td>
				<td width=5%>会议类型</td>
				<td width=10%>${meet.meetType}</td>
			</tr>
			<tr>
				<td>会议时间</td>
				<td>${meet.staTime?string('yyyy-MM-dd HH:mm')} 至
					${meet.endTime?string('HH:mm')} <input type='hidden' id='staTime'
					value="${meet.staTime?string('yyyy-MM-dd HH:mm')}"> <input
					type='hidden' id='endTime'
					value="${meet.endTime?string('yyyy-MM-dd HH:mm')}">
				</td>
				<td>状态</td>
				<td><#if meet.meetState=="已取消">
						<font color=red>${meet.meetState}</font> <#else>
						${meet.meetState} </#if></td>
				<td>申请时间</td>
				<td>${meet.meetCreateTime?string('yyyy-MM-dd
					HH:mm:ss')}</td>
			</tr>
			<#if meet.meetType=="视频会议">
			<tr class="info">
				<td style="text-align: left; font-size: 15px" colspan="6">&nbsp;&nbsp;&nbsp;账号密码</td>
			</tr>
			<tr>
				<!-- <td>服务器地址</td>
						<td><input type="text" class="form-control" id="serverAdd"
							value="121.9.9.38" placeholder="服务器地址" required></td> -->
				<td>视频会议号</td>
				<td colspan="2">${meet.resTwo}</td>
				<td>视频会议密码</td>
				<td colspan="2">${meet.resTwo}#</td>
			</tr>
			</#if>
			<#list amrs as amr>
			<tr class="info">
				<td style="text-align: left; font-size: 15px" colspan="6">&nbsp;&nbsp;&nbsp;${amr_index+1}号参会方</td>
			</tr>
			<tr>
				<td>会议室名称</td>
				<td colspan="2">${amr.resFive}</td>
				<td>参会人员</td>
				<td colspan="2">${amr.personNames!}</td>
			</tr>
			</#list>
		</table>
	</div>
</body>
</html>