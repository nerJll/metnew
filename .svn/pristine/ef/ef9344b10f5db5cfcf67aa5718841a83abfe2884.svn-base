<#assign ctx=request.contextPath />
<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>中转站</title>
<meta http-equiv="X-UA-Compatible" content="IE=edge">
<meta name="viewport" content="width=device-width, initial-scale=1">
<link rel="stylesheet" type="text/css" href="${ctx}/bootstrap/css/bootstrap.min.css">
<style type="text/css">
#app_0{
	font-size:20px;
	color:blue;
}
img{
	width:40px;
	height:30px;
}
</style>
</head>
<body>
	<!-- header -->
	<div id='app-0' class='container-fluid'>
		<table class='table'>
			<tr>
				<td>
					<img alt="左边" v-bind:src="zbtpdz">
					{{ hyyd }}
				</td>
			</tr>
			<tr>
				<td>
					<img alt="左边" v-bind:src="zbtpdz">
					{{ wdhy }}
				</td>
			</tr>
		</table>
	</div>
<script type="text/javascript" src="${ctx}/easyui/jquery.min.js"></script>
<script type="text/javascript" src="${ctx}/bootstrap/js/bootstrap.min.js"></script>
<script type="text/javascript" src="${ctx}/js/vue/vue.min.js"></script>

<script>
	var app0 = new Vue({
		el : '#app-0',
		data : {
			axzb : '左边',
			zbtpdz : '${ctx}/images/left.png',
			hyyd : '会议预订',
			wdhy : '我的会议'
		}
	})
</script>
</body>
</html>